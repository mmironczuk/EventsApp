@model IEnumerable<EventsApp.Models.Place>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.province)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.city)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.placeType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.province)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.city)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.placeType)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PlaceId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PlaceId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PlaceId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
